<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shenyue.react.login.dao.RoleDao">
    <resultMap id="role" type="com.shenyue.react.login.entity.RoleEntity">
        <id column="id" property="id"/>
        <result column="rolename" property="roleName"/>
    </resultMap>
    <select id="selectById" resultMap="role">
    select * from role where id = #{id}
  </select>
    <select id="selectAll" resultMap="role">
        select * from role;
    </select>
    <insert id="insert" parameterType="role">
        insert into role(id,roleName) values (#{id},#{roleName});
    </insert>
</mapper>